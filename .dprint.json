{
  "typescript": {
    "lineWidth": 80,
    "quoteStyle": "preferSingle",
    "semiColons": "asi"
  },
  "json": {
  },
  "markdown": {
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.77.0.wasm",
    "https://plugins.dprint.dev/json-0.16.0.wasm",
    "https://plugins.dprint.dev/markdown-0.14.1.wasm",
    "https://plugins.dprint.dev/prettier-0.17.0.json@5e42cbabab9906ad32ffe1aa67f15824b1c39408840f0afeaddc495dd362e177"
  ]
}
